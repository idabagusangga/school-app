<!DOCTYPE html>
<html>
    <head>
        <title>
            
        </title>

    </head>
    <body>

        <table>
            <thead></thead>
            <tr>
              <th>ID</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Full Name</th>
              <th>Subject Id</th>
              <th>email</th>
            </tr>
        </thead>
            <tbody>
              
            <tr>
                <%dataTeachers.forEach(function(teacher){%>

              <td><%=teacher.id%></td>
              <td><%=teacher.first_name%></td>
              <td><%=teacher.last_name%></td>
              <td><%=teacher.fullName()%></td>
              <td><%=teacher.email%></td>
              <td><a href="/teachers/edit/<%=teacher.id%>">edit</td>
              <td><a href="/teachers/delete/<%=teacher.id%>">delete</td>
              
            </tr>
            <%})%>
        </tbody>

           
          <br>  
    </body>
</html>