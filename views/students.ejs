<!DOCTYPE html>
<html>
    <head>
        <title>
            
        </title>

    </head>
    <body>

        <table>
            <thead><a href="students/add">add students</a></thead>
            <tr>
              <th>ID</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Email</th>
            </tr>
        </thead>
            <tbody>
              
            <tr>
                <%dataStudents.forEach(function(student){%>

              <td><%=student.id%></td>
              <td><%=student.first_name%></td>
              <td><%=student.last_name%></td>
              <td><%=student.getFullName()%></td>
              <td><%=student.email%></td>
              <td><a href="/students/edit/<%=student.id%>">edit</td>
              <td><a href="/students/delete/<%=student.id%>">delete</td>
              
            </tr>
            <%})%>
        </tbody>
        

           
          <br>  
    </body>
</html>